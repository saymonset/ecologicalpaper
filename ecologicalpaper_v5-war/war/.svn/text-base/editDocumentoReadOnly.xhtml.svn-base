					action="#{Documento.listarDocumentos}">
					<h:graphicImage title="#{txt.mas}" url="#{conftxt.img_mas}" />
					<f:setPropertyActionListener
						target="#{Documento.doc_maestro}" value="#{Documento.doc_detalle.doc_maestro}" />
				</a4j:commandLink>
				<f:verbatim></f:verbatim>

