<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:p="http://primefaces.org/ui">


<ui:composition template="/templates/templatetree.xhtml">

	<ui:define name="title">
		
	</ui:define>

	<ui:define name="pie">
 
	</ui:define>

	<ui:define name="body">
		<h:outputStylesheet>
a.no-decor>img {
    border: none;
}
</h:outputStylesheet>



		<a4j:status onstart="#{rich:component('statPane')}.show()"
			onstop="#{rich:component('statPane')}.hide()" />

		<h:form id="form">
		
		<center>
			<h1><h:outputText styleClass="form" value="#{txt.svnmodulo}" /></h1>
			</center>
			
			<a4j:jsFunction name="remove" render="table" execute="@this"
				oncomplete="#{rich:component('confirmPane')}.hide();" />

			<rich:popupPanel id="statPane" autosized="true">
				<h:graphicImage title="#{ai}" url="#{conftxt.img_esperar}" alt="ai" />
				<h:outputText value="#{txt.esperar}" />
			</rich:popupPanel>

			<rich:popupPanel header="..." id="confirmPane"
				domElementAttachment="parent" width="800" height="400">
				<h:panelGrid columns="1">
					<rich:messages></rich:messages>

				</h:panelGrid>



				<h:outputText value="#{txt.deleteconfirma}" />


				<a4j:commandButton value="#{txt.delete}"
					action="#{viewSvnModulo.remove}" render="table"
					execute="confirmPane"
					oncomplete="if (#{facesContext.maximumSeverity==null}) {#{rich:component('confirmPane')}.hide();}" />

				<a4j:commandButton value="#{txt.cancelar}"
					onclick="#{rich:component('confirmPane')}.hide(); return false;" />


			</rich:popupPanel>

			<rich:popupPanel header="..." id="editPane"
				domElementAttachment="parent" width="800" height="400">
				<rich:messages></rich:messages>
				<h:panelGroup />
				<h:panelGroup />

				<rich:tabPanel switchType="client">

					<rich:tab header="#{txt.svn} #{txt.svntipoambiente}">

						<h:panelGrid columns="3" id="editGrid">


							<h:panelGrid columns="2">
								<h:panelGrid columns="3">

									<h:outputText id="selectOne1" value="#{txt.svnurlbase}"
										title="#{txt.svnurlbase}" />
									<h:panelGroup>
										<h:selectOneMenu value="#{svnModuloCliente2.svnUrlBase}"
											binding="#{svnModuloCliente2.selectOneMenu1}"
											converter="ConverSvnUrlBase">
											<f:selectItems value="#{DocumentoSvn.allSvnUrlBase}" />
											<f:ajax event="change" render="selectOne21 selectOne21x12"
												execute="@this" action="#{svnModuloCliente2.change}" />
										</h:selectOneMenu>
										<rich:message for="selectOne1" />
									</h:panelGroup>
									<h:panelGroup />


									<h:outputText value="#{txt.svnnombreaplicacion}"
										title="#{txt.svnnombreaplicacion}" />
									<h:panelGroup>
										<h:selectOneMenu id="selectOne21"
											value="#{viewSvnModulo.svnNombreAplicacion1}"
											binding="#{svnModuloCliente2.selectOneMenu3}"
											converter="ConverSvnNombreAplicacion">
											<f:selectItems
												value="#{svnModuloCliente2.allSvnNombreAplicacion}" />
											<f:ajax event="change" execute="@this"
												render="selectOne21x12"
												action="#{svnModuloCliente2.change2}" />
										</h:selectOneMenu>
										<rich:message for="selectOne21" />
									</h:panelGroup>
									<h:panelGroup />


									<h:outputText value="#{txt.svntipoambiente}"
										title="#{txt.svntipoambiente}" />
									<h:panelGroup>
										<h:selectOneMenu id="selectOne21x12"
											binding="#{svnModuloCliente2.selectOneMenu2}"
											value="#{viewSvnModulo.objectItem.svnTipoAmbiente}"
											converter="converSvnTipoAmbiente">
											<f:selectItems
												value="#{svnModuloCliente2.allSvnTipoAmbientesAux}" />
											<f:ajax event="change" render="table" execute="@this" />
										</h:selectOneMenu>
										<rich:message for="selectOne21x12" />
									</h:panelGroup>
									<h:panelGroup />



									<h:outputText value="#{txt.svnpatchcorto1}" />
									<h:panelGroup>
										<h:inputText id="campo1"
											value="#{viewSvnModulo.objectItem.pathCorto1}"
											required="true" size="90">
											<f:validateLength maximum="89" minimum="4" />
											<f:validator validatorId="caracteresinvalidos" />
										</h:inputText>
										<rich:message for="campo1" />
										<h:outputLabel styleClass="form"
											value="#{txt.svnpatchcorto1_1}" />
									</h:panelGroup>
									<h:panelGroup />


								</h:panelGrid>
							</h:panelGrid>
						</h:panelGrid>
					</rich:tab>

				</rich:tabPanel>

				<h:panelGroup />
				<h:panelGrid columns="2">
					<a4j:commandButton value="#{txt.guardar}"
						action="#{viewSvnModulo.editar}" render="table" execute="editPane"
						oncomplete="if (#{facesContext.maximumSeverity==null}) {#{rich:component('editPane')}.hide();}" />
					<a4j:commandButton value="#{txt.cancelar}"
						action="#{viewSvnModulo.aceptar}"
						onclick="#{rich:component('editPane')}.hide(); return false;" />
				</h:panelGrid>
				<h:panelGroup />
			</rich:popupPanel>


			<h:panelGrid columns="3">
				<h:outputText id="selectOnexx1" value="#{txt.svnurlbase}"
					title="#{txt.svnurlbase}" />
				<h:selectOneMenu value="#{viewSvnModulo.svnUrlBase}"
					binding="#{svnModuloCliente2.selectOneMenu1_aux}"
					converter="ConverSvnUrlBase">
					<f:selectItems value="#{DocumentoSvn.allSvnUrlBase}" />
					<f:ajax event="change" render="selectOne21x table" execute="@this"
						action="#{svnModuloCliente2.changeAux}" />
				</h:selectOneMenu>
				<rich:message for="selectOnexx1" />

				<h:outputText value="#{txt.svnnombreaplicacion}"
					title="#{txt.svnnombreaplicacion}" />
				<h:selectOneMenu id="selectOne21x"
					binding="#{svnModuloCliente2.selectOneMenu3Aux}"
					value="#{viewSvnModulo.svnNombreAplicacion1}"
					converter="ConverSvnNombreAplicacion">
					<f:selectItems
						value="#{svnModuloCliente2.allSvnNombreAplicacioRichfaces}" />
					<f:ajax event="change" render="table selectOne21x1" execute="@this"
						action="#{svnModuloCliente2.change2Aux}" />
				</h:selectOneMenu>
				<rich:message for="selectOne21x" />

				<h:outputText value="#{txt.svntipoambiente}"
					title="#{txt.svntipoambiente}" />
				<h:selectOneMenu id="selectOne21x1"
					value="#{viewSvnModulo.svnTipoAmbiente}"
					converter="converSvnTipoAmbiente">
					<f:selectItems value="#{svnModuloCliente2.allSvnTipoAmbientesAux}" />
					<f:ajax event="change" render="table" execute="@this" />
				</h:selectOneMenu>
				<rich:message for="selectOne21x1" />

			</h:panelGrid>

			<rich:tabPanel switchType="client">
				<rich:tab header="#{txt.svnmodulo}">
					<rich:dataTable value="#{viewSvnModulo.allObjectItems}" var="car"
						iterationStatusVar="it" id="table"
						rows="#{Utilidades.verNumeroDeRegistros}">
						<rich:column>
							<f:facet name="header">#</f:facet>
                #{it.index}
            </rich:column>
						<rich:column>
							<f:facet name="header">
								<h:outputText value="#{txt.svnpatchcorto1}" />
							</f:facet>
							<h:outputText value="#{car.pathCorto1}" />
						</rich:column>


						<rich:column>
							<f:facet name="header">
								<h:panelGrid columns="3">

									<a4j:commandLink styleClass="no-decor" render="editGrid"
										execute="@this"
										oncomplete="#{rich:component('editPane')}.show()">
										<h:graphicImage title="#{txt.new}" url="#{conftxt.img_new}" />
										<a4j:param value="#{it.index}"
											assignTo="#{viewSvnModulo.currentCarIndex}" />

										<a4j:param value="false" assignTo="#{viewSvnModulo.swEditar}" />

										<f:setPropertyActionListener
											target="#{viewSvnModulo.objectItem}"
											value="#{viewSvnModulo.objeto}" />
									</a4j:commandLink>


								</h:panelGrid>

							</f:facet>
							<h:panelGrid columns="3">
								<h:panelGroup>
									<h:panelGrid columns="1">
										<!--							<%/* new */%>-->
									</h:panelGrid>
								</h:panelGroup>
								<a4j:commandLink styleClass="no-decor" render="editGrid"
									execute="@this"
									oncomplete="#{rich:component('editPane')}.show()">
									<h:graphicImage title="#{txt.edit}" url="#{conftxt.img_editar}" />
									<a4j:param value="#{it.index}"
										assignTo="#{viewSvnModulo.currentCarIndex}" />
									<f:setPropertyActionListener
										target="#{viewSvnModulo.objectItem}" value="#{car}" />
									<f:setPropertyActionListener target="#{viewSvnModulo.swEditar}"
										value="true" />


								</a4j:commandLink>

								<a4j:commandLink styleClass="no-decor" execute="@this"
									render="@none"
									oncomplete="#{rich:component('confirmPane')}.show()">
									<h:graphicImage title="#{txt.delete}"
										url="#{conftxt.img_erase}" />
									<a4j:param value="#{it.index}"
										assignTo="#{viewSvnModulo.currentCarIndex}" />

									<f:setPropertyActionListener
										target="#{viewSvnModulo.objectItem}" value="#{car}" />

								</a4j:commandLink>

							</h:panelGrid>
						</rich:column>

						<f:facet name="footer">
							<rich:dataScroller page="#{viewSvnModulo.page}" />
						</f:facet>
					</rich:dataTable>
				</rich:tab>
			</rich:tabPanel>

		</h:form>






	</ui:define>
</ui:composition>
</html>