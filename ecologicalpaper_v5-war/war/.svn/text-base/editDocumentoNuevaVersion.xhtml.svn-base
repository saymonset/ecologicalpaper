<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:p="http://primefaces.org/ui">


<ui:composition template="/templates/templatetree.xhtml">

	<ui:define name="title">
		
	</ui:define>

	<ui:define name="pie">
 
	</ui:define>

	<ui:define name="body">
 






		 
			<h:panelGrid columns="1">
				<rich:messages></rich:messages>
			</h:panelGrid>
			<h:form>
 <center>	<h1><h:outputText styleClass="form" value="#{txt.doc_version_nueva}:#{Flows.treeNodoActual.nombre}" /></h1></center>

				<h:panelGrid columns="3" border="0">
 

					<h:outputText id="fileuploadxxx"
						rendered="#{newVersion.swExecuteActualizar}"
						value="#{txt.doc_datosCargaDocumentotabp} " />
					<p:fileUpload rendered="#{newVersion.swExecuteActualizar}"
						fileUploadListener="#{newVersion.handleFileUpload}"
						allowTypes="#{viewTreeDocDetalles.extensiones}"
						 />
						 <p:growl id="fileupload" showDetail="true" sticky="false"
						life="300000"/>
						
						
						


					<h:outputText value="#{txt.doc_razodelcambio}" />
					<h:inputTextarea cols="100" rows="10"  value="#{newVersion.doc_detalle.descripcion}"
						/>
					<rich:message for="desc2" showDetail="true" />

					<h:panelGrid column="1"></h:panelGrid>
					<h:panelGrid column="2">
						<h:commandLink id="newVersion"
							rendered="#{!newVersion.swExecuteActualizar}"
							action="#{newVersion.nuevaVersionDoc}"
							onclick="this.style.display='none';">
							<h:panelGrid columns="2">
								<h:graphicImage id="imageNewVersion"
									title="#{txt.doc_version_nueva}"
									url="#{conftxt.img_newversion}" />
								<h:outputText value="#{txt.doc_version_nueva}" />
							</h:panelGrid>
						</h:commandLink>
						<h:commandLink id="update"
							rendered="#{newVersion.swExecuteActualizar}"
							action="#{newVersion.actualizarDoc}"
							onclick="this.style.display='none';">
							<h:panelGrid columns="2">
								<h:graphicImage 
								styleClass="no-decor"
								id="imageUpdate" title="#{txt.doc_actualiza}"
									url="#{conftxt.img_update}" />
								<h:outputText value="#{txt.doc_version_nueva} xxxx" />
							</h:panelGrid>

						</h:commandLink>
					</h:panelGrid>
					<h:panelGrid column="1"></h:panelGrid>



				</h:panelGrid>
			</h:form>
			<h:form id="form2">
				<h:panelGrid columns="2" border="0">


						<a4j:commandButton value="#{txt.btn_menu}"
						action="#{newVersion.cancelar}"
						onclick="this.style.display='none';" />
					<!--			<%-->
					<!--				/*  Esro solo lo qu hace es actualizar el mismo documento */-->
					<!--			%>-->
				</h:panelGrid>
			</h:form>



 


	</ui:define>
</ui:composition>
</html>