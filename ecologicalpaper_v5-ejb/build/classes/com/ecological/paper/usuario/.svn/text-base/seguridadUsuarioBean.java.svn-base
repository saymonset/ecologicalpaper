package com.ecological.paper.usuario;

import com.ecological.paper.tree.Tree;
import java.util.List;

/**
 *
 * @author simon
 */
public class seguridadUsuarioBean {
    private Tree tree;
    private List operaciones;
    
    /** Creates a new instance of seguridadUsuarioBean */
    public seguridadUsuarioBean() {
    }

     public seguridadUsuarioBean(Tree tree , List operaciones) {
         this.setTree(tree);
         this.setOperaciones(operaciones);
    }
    public Tree getTree() {
        return tree;
    }

    public void setTree(Tree tree) {
        this.tree = tree;
    }

    public List getOperaciones() {
        return operaciones;
    }

    public void setOperaciones(List operaciones) {
        this.operaciones = operaciones;
    }

 
}
