package com.ecological.paper.ecologicaexcepciones;

public class EcologicaExcepcionesContextType extends Exception{
    
    /** Creates a new instance of EcologicaExcepciones */
    public EcologicaExcepcionesContextType() {
        this("EcologicaExcepciones");
    }
    public EcologicaExcepcionesContextType(String msg) {
        super(msg);
    }
    
    public EcologicaExcepcionesContextType(Throwable t){
        super(t);
    }
    
    public EcologicaExcepcionesContextType(String msg, Throwable t){
        super(msg,t);
    }
}