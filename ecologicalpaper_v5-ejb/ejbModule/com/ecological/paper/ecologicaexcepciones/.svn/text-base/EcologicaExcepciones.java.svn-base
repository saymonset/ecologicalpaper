package com.ecological.paper.ecologicaexcepciones;

/**
 *
 * @author simon
 */
public class EcologicaExcepciones extends Exception{
    
    /** Creates a new instance of EcologicaExcepciones */
    public EcologicaExcepciones() {
        this("EcologicaExcepciones");
    }
    public EcologicaExcepciones(String msg) {
        super(msg);
    }
    
    public EcologicaExcepciones(Throwable t){
        super(t);
    }
    
    public EcologicaExcepciones(String msg, Throwable t){
        super(msg,t);
    }
    
}
